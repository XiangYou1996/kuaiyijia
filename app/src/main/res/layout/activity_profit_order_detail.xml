<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/backtolast"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="25dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#333333"
        android:drawableLeft="@drawable/icon_back"
        />
    <LinearLayout
        android:id="@+id/profit_detail"
        android:layout_alignParentStart="true"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:orientation="vertical"
        android:background="#333333"
        android:gravity="center"
        tools:ignore="MissingConstraints">
        <TextView
            android:id="@+id/profit_detail_person"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="订单收益详情"
            android:textSize="30sp"
            android:textColor="@color/white" />
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="30dp"
        android:layout_above="@+id/profit_detail_more_confirm_bt"
        android:layout_below="@+id/profit_detail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#feeeed">
        <TextView
            android:id="@+id/ordernum_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="订单号："
            android:textSize="18sp"
            android:textColor="#000"/>
        <ListView
            android:id="@+id/ordermore_list"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </LinearLayout>
    <Button
        android:id="@+id/profit_detail_more_confirm_bt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:text="确认"
        android:textSize="20dp"/>
</RelativeLayout>